{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Run Current Streamlit App",
        "type": "debugpy",
        "request": "launch",
        "module": "streamlit",
        "args": ["run", "src/main.py"],
        "justMyCode": false,
        "env": {"no_proxy": "localhost,192.168.1.1,127.0.0.0,127.0.0.1,127.0.1.1,127.0.1.1,local.home"}
      },
      {
        "name": "Run CLI agent",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}/src/agent_cli.py",
        "console": "integratedTerminal",
        "env": {"no_proxy": "localhost,192.168.1.1,127.0.0.0,127.0.0.1,127.0.1.1,127.0.1.1,local.home"}
      },
      {
        "name": "Run file",
        "type": "debugpy",
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal",
      },
    ]
  }